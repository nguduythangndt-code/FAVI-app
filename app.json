{
  "expo": {
    "name": "FAVI",
    "slug": "livestock-care",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favi-logo.png",
    "userInterfaceStyle": "light",
    "scheme": "favi",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/favi-favi.png",
      "backgroundColor": "#F0F9F4",
      "resizeMode": "contain"
    },
    "android": {
      "package": "com.favi.livestock",
      "versionCode": 2,
      "icon": "./assets/images/favi-logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favi-logo.png",
        "backgroundColor": "#C9FFE9"
      },
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/favi-logo.png",
        "backgroundColor": "#F0F9F4",
        "resizeMode": "contain"
      }
    },
    "ios": {
      "bundleIdentifier": "com.favi.livestock",
      "supportsTablet": false,
      "icon": "./assets/images/favi-logo.png",
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/favi-logo.png",
        "backgroundColor": "#F0F9F4",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "targetSdkVersion": 35
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dc4387ce-1706-48d1-a11a-a2f6f8da4ed5"
      }
    }
  }
}
